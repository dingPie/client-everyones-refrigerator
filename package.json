{
  "name": "EveryonesRefrigerator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=developmentDebug",
    "android:staging": "react-native run-android --variant=stagingDebug",
    "android:production": "react-native run-android --variant=productionDebug",
    "ios": "react-native run-ios --scheme development",
    "ios:staging": "react-native run-ios --scheme staging",
    "ios:production": "react-native run-ios --scheme production",
    "start": "node_modules/.bin/ls-engines && react-native start",
    "test": "jest --coverage --coverageReporters=text-summary",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tokript": "rn-tokript",
    "gen:source": "rn-tokript gen:source",
    "gen:img": "rn-tokript gen:img",
    "gen:font": "rn-tokript gen:font",
    "gen:api": "rn-tokript gen:api",
    "gen:icon": "cd src/scripts && node genIcons.js",
    "w:start": "rn-tokript w:start",
    "commit": "rn-tokript commit",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "clean": "cd android && ./gradlew clean && rm -rf ~/Library/Developer/Xcode/DerivedData && cd .. && cd ios && rm -rf Podfile.lock && pod update && pod install",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.7",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/messaging": "^14.11.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@tanstack/react-query": "^4.2.3",
    "axios": "^1.3.4",
    "immer": "^10.0.2",
    "ls-engines": "^0.7.0",
    "native-base": "^3.4.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.34.0",
    "react-native": "0.70.0",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.11.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-redux": "^8.0.1",
    "tinycolor2": "^1.4.2",
    "use-context-selector": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/devtools": "^6.0.18",
    "@testing-library/jest-native": "^5.4.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.7.15",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.4",
    "eslint": "^7.32.0",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "git-cz": "^4.8.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-dotenv": "^3.4.8",
    "react-native-flipper": "^0.171.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-query-native-devtools": "^4.0.0",
    "react-test-renderer": "^18.2.0",
    "redux-flipper": "^2.0.2",
    "rn-flipper-async-storage-advanced": "^1.0.4",
    "rn-tokript": "0.9.6",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "engines": {
    "node": ">= 16.10",
    "npm": ">= 7.10.0",
    "yarn": ">= 1.22.17"
  },
  "os": [
    "darwin",
    "win64",
    "linux"
  ],
  "engineStrict": true
}
